{
  "expo": {
    "name": "Estoque Orto baby",
    "slug": "estoque",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ortobaby/icone.png",
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ]
    ],
    "splash": {
      "image": "./assets/ortobaby/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e6e6e6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ortobaby/icone.png",
        "backgroundColor": "#e6e6e6"
      },
      "package": "com.piska.estoque"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
